{
	"name": "boilerplate",
	"description": "Boilerplate: Typescript, Webpack, Isomorphic React, Sass, Handlebars, Material UI",
	"version": "3.0.0",
	"main": "server.ts",
	"author": "Fun Maker",
	"repository": "https://github.com/funmaker/webapp-boilerplate",
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=7.0.0"
	},
	"scripts": {
		"start": "rm -rf build && webpack --config webpack/server.dev.mjs",
		"start:prod": "cd dist && npm start",
		"build:prod": "mkdir -p dist && npm-run-all build:prod-*",
		"build:prod-server": "webpack --config webpack/server.prod.mjs",
		"build:prod-client": "webpack --config webpack/client.prod.mjs",
		"build:prod-static": "cp -rL static dist/",
		"build:prod-package": "babel-node webpack/package.mjs",
		"lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --max-warnings 0 --ignore-path .gitignore .",
		"lint-fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-path .gitignore . --fix"
	},
	"dependencies": {
		"@emotion/styled": "^11.11.0",
		"@mui/material": "^5.15.5",
		"@mui/styled-engine": "^5.15.5",
		"axios": "^1.6.5",
		"body-parser": "^1.20.2",
		"chalk": "^4.1.2",
		"compression": "^1.7.4",
		"connect-pg-simple": "^9.0.1",
		"cookie-parser": "^1.4.6",
		"core-js": "^3.35.1",
		"csurf": "^1.11.0",
		"detect-node": "^2.1.0",
		"express": "^4.18.2",
		"express-handlebars": "^7.1.2",
		"express-promise-router": "^4.1.1",
		"express-session": "^1.17.3",
		"lorem-ipsum": "^2.0.8",
		"morgan": "^1.10.0",
		"pg": "^8.11.3",
		"prop-types": "^15.8.1",
		"qs": "^6.11.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router": "^6.21.3",
		"react-router-dom": "^6.21.3",
		"react-toastify": "^10.0.4",
		"resize-observer-polyfill": "^1.5.1",
		"source-map-support": "^0.5.21",
		"sql-template-strings": "^2.2.2",
		"use-debounce": "^10.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.23.7",
		"@babel/node": "^7.22.19",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-react": "^7.23.3",
		"@babel/register": "^7.23.7",
		"@emotion/babel-plugin": "^11.11.0",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
