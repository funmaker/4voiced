		"@types/compression": "^1.7.5",
		"@types/connect-pg-simple": "^7.0.3",
		"@types/cookie-parser": "^1.4.6",
		"@types/csurf": "^1.11.5",
		"@types/detect-node": "^2.0.2",
		"@types/express": "^4.17.21",
		"@types/express-session": "^1.17.10",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.5",
		"@types/pg": "^8.10.9",
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
